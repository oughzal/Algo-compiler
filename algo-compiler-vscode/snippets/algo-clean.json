{
    "Algorithme": {
        "prefix": "algo",
        "body": [
            "algorithme ${1:NomAlgorithme}",
            "",
            "variables",
            "    ${2:a} : ${3:entier}",
            "",
            "debut",
            "    $0",
            "fin"
        ],
        "description": "Structure de base d'un algorithme"
    },
    "Fonction": {
        "prefix": "fonction",
        "body": [
            "fonction ${1:nom}(${2:param} : ${3:entier}) : ${4:entier}",
            "debut",
            "    $0",
            "    retourner ${5:resultat}",
            "fin"
        ],
        "description": "Définir une fonction"
    },
    "Procédure": {
        "prefix": "procedure",
        "body": [
            "procedure ${1:nom}(${2:param} : ${3:entier})",
            "debut",
            "    $0",
            "fin"
        ],
        "description": "Définir une procédure"
    },
    "Si Alors": {
        "prefix": "si",
        "body": [
            "si ${1:condition} alors",
            "    $0",
            "finsi"
        ],
        "description": "Structure conditionnelle si"
    },
    "Si Alors Sinon": {
        "prefix": "sisi",
        "body": [
            "si ${1:condition} alors",
            "    ${2}",
            "sinon",
            "    $0",
            "finsi"
        ],
        "description": "Structure conditionnelle si-sinon"
    },
    "Si SinonSi Sinon": {
        "prefix": "sinonsi",
        "body": [
            "si ${1:condition1} alors",
            "    ${2}",
            "sinonSi ${3:condition2} alors",
            "    ${4}",
            "sinon",
            "    $0",
            "finsi"
        ],
        "description": "Structure si avec sinonSi"
    },
    "SinonSi clause": {
        "prefix": "sinsi",
        "body": [
            "sinonSi ${1:condition} alors",
            "    $0"
        ],
        "description": "Clause sinonSi"
    },
    "Expression conditionnelle": {
        "prefix": "siexpr",
        "body": [
            "${1:variable} = si ${2:condition} alors ${3:valeur1} sinon ${4:valeur2}"
        ],
        "description": "Expression conditionnelle (opérateur ternaire)"
    },
    "Pour": {
        "prefix": "pour",
        "body": [
            "pour ${1:i} de ${2:0} à ${3:10} faire",
            "    $0",
            "finpour"
        ],
        "description": "Boucle pour"
    },
    "Pour avec Pas": {
        "prefix": "pourpas",
        "body": [
            "pour ${1:i} de ${2:0} à ${3:10} pas ${4:1} faire",
            "    $0",
            "finpour"
        ],
        "description": "Boucle pour avec pas (step)"
    },
    "Tantque": {
        "prefix": "tantque",
        "body": [
            "tantque ${1:condition} faire",
            "    $0",
            "fintantque"
        ],
        "description": "Boucle tantque"
    },
    "Repeter": {
        "prefix": "repeter",
        "body": [
            "repeter",
            "    $0",
            "jusqua ${1:condition}"
        ],
        "description": "Boucle repeter-jusqua"
    },
    "Selon": {
        "prefix": "selon",
        "body": [
            "selon(${1:expression})",
            "    cas ${2:valeur1}: ${3}",
            "    cas ${4:valeur2}: ${5}",
            "    defaut: $0",
            "finselon"
        ],
        "description": "Structure selon (switch)"
    },
    "Cas": {
        "prefix": "cas",
        "body": [
            "cas ${1:valeur}: $0"
        ],
        "description": "Un cas dans une structure selon"
    },
    "Defaut": {
        "prefix": "defaut",
        "body": [
            "defaut: $0"
        ],
        "description": "Cas par défaut dans selon"
    },
    "Variables": {
        "prefix": "var",
        "body": [
            "${1:nom} : ${2:entier}"
        ],
        "description": "Déclarer une variable"
    },
    "Variable avec initialisation": {
        "prefix": "varinit",
        "body": [
            "${1:nom} : ${2:entier} = ${3:valeur}"
        ],
        "description": "Déclarer et initialiser une variable"
    },
    "Tableau": {
        "prefix": "tableau",
        "body": [
            "${1:tab} : tableau[${2:10}] de ${3:entier}"
        ],
        "description": "Déclarer un tableau 1D"
    },
    "Matrice": {
        "prefix": "matrice",
        "body": [
            "${1:M} : tableau[${2:3}][${3:3}] de ${4:entier}"
        ],
        "description": "Déclarer une matrice (tableau 2D)"
    },
    "Tableau littéral": {
        "prefix": "tablit",
        "body": [
            "${1:tableau} = [${2:1}, ${3:2}, ${4:3}]"
        ],
        "description": "Initialiser un tableau avec littéral"
    },
    "Constantes": {
        "prefix": "const",
        "body": [
            "constantes",
            "    ${1:NOM} : ${2:entier} = ${3:valeur}"
        ],
        "description": "Bloc de déclaration de constantes"
    },
    "Ecrire": {
        "prefix": "ecrire",
        "body": [
            "ecrire(${1:message})"
        ],
        "description": "Afficher sans retour à la ligne"
    },
    "Ecrireln": {
        "prefix": "ecrireln",
        "body": [
            "ecrireln(${1:message})"
        ],
        "description": "Afficher avec retour à la ligne"
    },
    "Lire": {
        "prefix": "lire",
        "body": [
            "lire(${1:variable})"
        ],
        "description": "Lire une valeur"
    },
    "Fonction abs": {
        "prefix": "abs",
        "body": [
            "abs()"
        ],
        "description": "Valeur absolue"
    },
    "Fonction min": {
        "prefix": "min",
        "body": [
            "min()"
        ],
        "description": "Minimum de deux valeurs"
    },
    "Fonction max": {
        "prefix": "max",
        "body": [
            "max()"
        ],
        "description": "Maximum de deux valeurs"
    },
    "Fonction racine": {
        "prefix": "racine",
        "body": [
            "racine()"
        ],
        "description": "Racine carrée"
    },
    "Fonction puissance": {
        "prefix": "puissance",
        "body": [
            "puissance()"
        ],
        "description": "Élever à la puissance"
    },
    "Fonction arrondi": {
        "prefix": "arrondi",
        "body": [
            "arrondi()"
        ],
        "description": "Arrondir un nombre"
    },
    "Fonction longueur": {
        "prefix": "longueur",
        "body": [
            "longueur()"
        ],
        "description": "Longueur d'une chaîne"
    },
    "Fonction majuscule": {
        "prefix": "majuscule",
        "body": [
            "majuscule()"
        ],
        "description": "Convertir en majuscules"
    },
    "Fonction minuscule": {
        "prefix": "minuscule",
        "body": [
            "minuscule()"
        ],
        "description": "Convertir en minuscules"
    },
    "Fonction contient": {
        "prefix": "contient",
        "body": [
            "contient()"
        ],
        "description": "Vérifie si texte contient sous-chaîne"
    },
    "Fonction estVide": {
        "prefix": "estvide",
        "body": [
            "estVide()"
        ],
        "description": "Vérifie si chaîne est vide"
    },
    "Fonction position": {
        "prefix": "position",
        "body": [
            "position()"
        ],
        "description": "Trouver position d'une sous-chaîne"
    },
    "Fonction ord": {
        "prefix": "ord",
        "body": [
            "ord()"
        ],
        "description": "Caractère vers code ASCII"
    },
    "Fonction chr": {
        "prefix": "chr",
        "body": [
            "chr()"
        ],
        "description": "Code ASCII vers caractère"
    },
    "Fonction estLettre": {
        "prefix": "estlettre",
        "body": [
            "estLettre()"
        ],
        "description": "Vérifie si c'est une lettre"
    },
    "Fonction estChiffre": {
        "prefix": "estchiffre",
        "body": [
            "estChiffre()"
        ],
        "description": "Vérifie si c'est un chiffre"
    },
    "Fonction versMajuscule": {
        "prefix": "versmajuscule",
        "body": [
            "versMajuscule()"
        ],
        "description": "Convertir caractère en majuscule"
    },
    "Fonction versMinuscule": {
        "prefix": "versminuscule",
        "body": [
            "versMinuscule()"
        ],
        "description": "Convertir caractère en minuscule"
    },
    "Fonction versEntier": {
        "prefix": "versentier",
        "body": [
            "versEntier()"
        ],
        "description": "Convertir en entier"
    },
    "Fonction versReel": {
        "prefix": "versreel",
        "body": [
            "versReel()"
        ],
        "description": "Convertir en réel"
    },
    "Fonction versChaine": {
        "prefix": "verschaine",
        "body": [
            "versChaine()"
        ],
        "description": "Convertir en chaîne"
    },
    "Fonction aleatoire": {
        "prefix": "aleatoire",
        "body": [
            "aleatoire()"
        ],
        "description": "Nombre aléatoire"
    },
    "Division entière": {
        "prefix": "div",
        "body": [
            "${1:a} div ${2:b}"
        ],
        "description": "Division entière"
    },
    "Modulo": {
        "prefix": "mod",
        "body": [
            "${1:a} mod ${2:b}"
        ],
        "description": "Opérateur modulo"
    },
    "Puissance": {
        "prefix": "puis",
        "body": [
            "${1:base} ^ ${2:exposant}"
        ],
        "description": "Opérateur puissance ^"
    },
    "Parcours matrice": {
        "prefix": "parcmat",
        "body": [
            "pour ${1:i} de 0 à ${2:lignes} - 1 faire",
            "    pour ${3:j} de 0 à ${4:colonnes} - 1 faire",
            "        ${5:M}[${1:i}][${3:j}] = $0",
            "    finpour",
            "finpour"
        ],
        "description": "Parcourir une matrice"
    },
    "Afficher matrice": {
        "prefix": "affmat",
        "body": [
            "pour ${1:i} de 0 à ${2:lignes} - 1 faire",
            "    pour ${3:j} de 0 à ${4:colonnes} - 1 faire",
            "        ecrire(${5:M}[${1:i}][${3:j}], \" \")",
            "    finpour",
            "    ecrireln()",
            "finpour"
        ],
        "description": "Afficher une matrice"
    }
}

